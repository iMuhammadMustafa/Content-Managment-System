<h1>Users</h1>

<table>
  <tr>
    <th>Username</th>
    <th>Email</th>
    <th>Role</th>
    <th>Actions</th>
  </tr>
  <% users.forEach(function(user) { %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= user.role %></td>
    <td>
      <a href="/admin/users/edit/<%= user.id %>">Edit</a>
      <a href="/admin/users/delete/<%= user.id %>">Delete</a>
    </td>
  </tr>
  <% }); %>
</table>
